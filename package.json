  "dependencies": {
    "axios": "^0.21.1",
    "bufferutil": "^4.0.1",
    "discord.js": "^12.2.0",
    "discord.js-commando": "github:discordjs/Commando",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.0.0",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "^5.0.2",
